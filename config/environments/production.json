{
  "front-end": {
    "protocol": "http",
    "host": "localhost",
    "port": 8000,
    "successMessageAutoHideDuration": 3000
  },

  "database": {
    "redis": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },

  "services": {
    "logs": {
      "host": "localhost",
      "defaultPort": 3100,
      "totalInitialInstances": 1,
      "publiclyAccessedRoutes": [
        "/api/v1/logs"
      ],
      "routes": {
        "system": {
          "apiPathPrefix": "/api/v1"
        }
      }
    },

    "auth": {
      "host": "localhost",
      "defaultPort": 3200,
      "totalInitialInstances": 1,
      "publiclyAccessedRoutes": [
        "/api/v1/auth"
      ],
      "jwtSecretsFileName": "jwt",
      "maxTokenLifeTimestamp": 259200000,
      "userPasswordSecretsFileName": "user-password",
      "routes": {
        "default": {
          "apiPathPrefix": "/api/v1"
        }
      }
    },

    "roles": {
      "host": "localhost",
      "defaultPort": 3300,
      "totalInitialInstances": 1,
      "publiclyAccessedRoutes": [
        "/api/v1/roles"
      ],
      "routes": {
        "default": {
          "apiPathPrefix": "/api/v1"
        }
      }
    }
  }
}